<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
    <ribbon>
        <tabs>
            <tab id="tabPalo" label="Palo">
                <group id="grpPalo" label="Palo Functions">
                    <button id="btnModeller" onAction="btnModeller_Click" label="Modeller" size="large" />
                    <separator id="separator1" />
                    <button id="btnPasteView" onAction="btnPasteView_Click" label="Paste View" size="large" />
                    <button id="btnPasteElement" onAction="btnPasteElement_Click" label="Paste Elements" size="large" />
                    <separator id="separator2" />
                    <button id="btnSaveSnapshot" onAction="btnSaveSnapshot_Click" label="Snapshot" size="large" />
                    <separator id="separator3" />
                    <button id="btnAbout" onAction="btnAbout_Click" label="About Palo" size="large" />
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>